  <DataSets>
    <DataSet Name="Reporte_ProdutoMasVendido">
      <Query>
        <DataSourceName>PosPF_DB</DataSourceName>
        <CommandText>SELECT Ppv.Nombre, SUM(Ppv.Cantidad) AS Total 
FROM View_ProductoPorVenta AS Ppv
WHERE Ppv.Estado='A'
GROUP BY Ppv.Nombre</CommandText>
      </Query>
      <Fields>
